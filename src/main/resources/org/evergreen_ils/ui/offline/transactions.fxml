<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="724.0" prefWidth="655.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.evergreen_ils.ui.offline.TransactionsController">
   <children>
      <GridPane prefHeight="722.0" prefWidth="636.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="652.0" minWidth="10.0" prefWidth="652.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="320.0" minWidth="0.0" prefWidth="0.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="141.0" minHeight="1.0" prefHeight="109.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="673.0" minHeight="10.0" prefHeight="527.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="257.0" minHeight="0.0" prefHeight="103.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" onAction="#exportTransactions" text="Export Transactions" GridPane.halignment="RIGHT">
               <GridPane.margin>
                  <Insets right="10.0" />
               </GridPane.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#deleteTransactions" text="Delete Transactions" GridPane.halignment="RIGHT" GridPane.rowIndex="2">
               <GridPane.margin>
                  <Insets right="10.0" />
               </GridPane.margin>
            </Button>
            <TableView prefHeight="527.0" prefWidth="652.0" GridPane.rowIndex="1" fx:id="xactsTable">
              <columns>
                <TableColumn fx:id="realTimeCol" prefWidth="54.0" text="Time" />
                <TableColumn fx:id="actionCol" prefWidth="78.0" text="Action" />
                <TableColumn fx:id="patronBarcodeCol" prefWidth="145.0" text="Patron" />
                <TableColumn fx:id="itemBarcodeCol" prefWidth="103.0" text="Item" />
                <TableColumn fx:id="nonCatCountCol" prefWidth="63.0" text="Count" />
                <TableColumn fx:id="dueDateCol" minWidth="0.0" prefWidth="104.0" text="Due Date" />
                  <TableColumn fx:id="backDateCol" prefWidth="104.0" text="Backdate" />
              </columns>
            </TableView>
         </children>
      </GridPane>
   </children>
</VBox>
